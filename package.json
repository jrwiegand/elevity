  "bugs": {
    "url": "https://github.com/jrwiegand/elevity/issues"
  },
  "homepage": "https://github.com/jrwiegand/elevity#readme",
  "devDependencies": {
    "@11ty/eleventy": "2.0.0-beta.2",
    "@11ty/eleventy-plugin-vite": "4.0.0",
    "vite": "4.0.4"
  }
}
